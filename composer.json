{
  "name": "louisk/boleto-facil",
  "description": "Boleto Facil integration with PHP and Laravel",
  "keywords": [
    "boleto-facil",
    "boleto",
    "facil",
    "php",
    "laravel"
  ],
  "homepage": "https://github.com/keviinlouis/BoletoFacil",
  "support": {
    "issues": "https://github.com/keviinlouis/BoletoFacil/issues",
    "source": "https://github.com/keviinlouis/BoletoFacil"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Louis",
      "email": "keviinlouiis@gmail.com",
      "homepage": "https://github.com/keviinlouis",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
    "guzzlehttp/guzzle": "~6.0",
    "nesbot/carbon": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8 || ~6.0"
  },
  "autoload": {
    "psr-4": {
      "Louisk\\BoletoFacil\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    },
    "laravel": {
      "aliases": {
        "BoletoFacil": "Louisk\\BoletoFacil\\Laravel\\Facades\\BoletoFacilFactory"
      },
      "providers": [
        "Louisk\\BoletoFacil\\Laravel\\BoletoFacilServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
